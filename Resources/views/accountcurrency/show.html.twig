{% extends '@MetalslaveFinancial/base.html.twig' %}

{% block body %}
    <h1>Accountcurrency</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ accountCurrency.id }}</td>
            </tr>
            <tr>
                <th>Shortname</th>
                <td>{{ accountCurrency.shortName }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ accountCurrency.name }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('account_currency_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('account_currency_edit', { 'id': accountCurrency.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
